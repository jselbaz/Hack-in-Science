def list_pretty_print(items):
    output = ""
    count = 0
    for i in items:
        count += 1
        if count % 5 == 0:
            output += str(i) + '\n'
            continue
        output += str(i) + ', '
    if count % 5 != 0:
        print(output[:-2])
    else:
        print(output)
    
